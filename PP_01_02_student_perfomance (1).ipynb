        "#1\n",
        "import csv\n",
        "\n",
        "fieldnames = ['ID', 'Name', 'Math', 'English', 'Science', 'Grade']\n",
        "\n",
        "student_data = [\n",
        "    [1, 'Alice', 95, 88, 92, ''],\n",
